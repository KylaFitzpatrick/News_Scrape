
    <div class="row">
        <div class="col s12">
            <div class="container-fluid">
                <div class="centered">
                    <h1>Mongo Scraper</h1>
                    <p>SF Chronicle Edition</p>
                </div>
            </div>
            <img src="./images/san-francisco-free-photo-picjumbo-com.jpg" class="responsive-img">

        </div>
    </div>

    <div id="articles">
        {{#if article}}
        {{#each article}}
        <div data-id="{{this._id}}" class='row'> 
            <div class='col s12'> 
                <div class='card black darken-1'> <div class='card-content white-text'> <span class='card-title'>{{this.title}}</span>{{this.summary}} <br /> <a target="_blank" href="{{this.link}}"></a>
                </div> 
                <div class='card-action'> 
                    <a data-id="{{this._id}}" class='waves-effect waves-light btn save'>Save Article</a>
                    </div> 
                    </div> 
                    </div> 
                    </div>
    </div>
    {{/each}}
    {{else}}
    <div id="notes">
        <h4>Oops! There are no new articles.</h4>
    </div>
    {{/if}}
  

